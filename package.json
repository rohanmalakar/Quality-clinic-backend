{
   "name": "dsn-backend",
   "version": "0.0.1",
   "description": "backend for dsn project",
   "main": "dist/app.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "dev": "nodemon",
      "start": "ts-node -r tsconfig-paths/register src/app.ts",
      "build": "tsc && tsc-alias",
      "start:prod": "node dist/app.js",
      "encryption": "ts-node src/scripts/encryption.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:generate": "npm run typeorm -- -d src/models/data-source.ts migration:generate src/models/migrations/$npm_config_name",
      "migration:create": "npm run typeorm migration:create src/models/migrations/$npm_config_name",
      "migration:run": "npm run typeorm -- -d src/models/data-source.ts migration:run",
      "migration:revert": "npm run typeorm -- -d src/models/data-source.ts migration:revert"
   },
   "nodemonConfig": {
      "ignore": [
         "**/*.test.ts",
         "**/*.spec.ts",
         ".git",
         "node_modules"
      ],
      "watch": [
         "src"
      ],
      "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/app.ts",
      "ext": "ts, js"
   },
   "author": "DSN Limited",
   "license": "ISC",
   "dependencies": {
      "@aws-sdk/client-s3": "^3.758.0",
      "@aws-sdk/types": "^3.734.0",
      "@turf/helpers": "^6.5.0",
      "@turf/turf": "^6.5.0",
      "@types/bcrypt": "^5.0.2",
      "@types/bcryptjs": "^2.4.6",
      "@types/morgan": "^1.9.3",
      "@types/mysql": "^2.15.25",
      "axios": "^1.9.0",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.19.1",
      "cors": "^2.8.5",
      "dotenv": "^16.5.0",
      "express": "^4.21.2",
      "ioredis": "^5.6.1",
      "jsonwebtoken": "^9.0.2",
      "lodash": "^4.17.21",
      "morgan": "^1.10.0",
      "multer": "1.4.5-lts.1",
      "mysql2": "^3.8.0",
      "paytabs_pt2": "^2.0.11",
      "redis": "^4.7.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.19",
      "winston": "^3.5.0",
      "ws": "^8.2.3",
      "zod": "^3.14.4"
   },
   "devDependencies": {
      "@types/amqplib": "^0.8.2",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/ioredis": "^4.28.7",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/lodash": "^4.14.182",
      "@types/multer": "^1.4.12",
      "@types/node": "^16.18.126",
      "@types/ws": "^8.2.0",
      "nodemon": "^3.1.10",
      "ts-node": "10.9.1",
      "tsc-alias": "^1.8.8",
      "tsconfig-paths": "^3.15.0",
      "typescript": "^5.9.2"
   }
}
